@page
@using Microsoft.Extensions.Localization
@using QuanHuyenModule.Localization
@model QuanHuyenModule.Web.Pages.QuanHuyens.IndexModel
@inherits QuanHuyenModule.Web.Pages.QuanHuyenModulePage

@section scripts
{
    <abp-script src="/Pages/QuanHuyens/Index.js"/>
}
    <abp-card>
        <abp-card-header>
            <abp-row>
                <abp-column size-md="_6">
                    <abp-card-title>@L["QuanHuyenModule"]</abp-card-title>
                </abp-column>
                <abp-column size-md="_6" class="text-end">
                    <abp-button id="NewQuanHuyenButton"
                            text="@L["NewQuanHuyen"].Value"
                            icon="plus"
                            button-type="Primary" />
                </abp-column>
            </abp-row>
        </abp-card-header>
        <abp-card-body>
                <abp-table striped-rows="true" id="QuanHuyenTable"></abp-table>
        </abp-card-body>
    </abp-card>